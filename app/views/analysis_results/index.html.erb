<p id="notice"><%= notice %></p>

<h1>Analysis Results</h1>

<table>
  <thead>
    <tr>
      <th>Sentence</th>
      <th>Sentiment label</th>
      <th>Sentiment score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analysis_results.each do |analysis_result| %>
      <tr>
        <td><%= analysis_result.sentence_id %></td>
        <td><%= analysis_result.sentiment_label %></td>
        <td><%= analysis_result.sentiment_score %></td>
        <td><%= link_to 'Show', analysis_result %></td>
        <td><%= link_to 'Edit', edit_analysis_result_path(analysis_result) %></td>
        <td><%= link_to 'Destroy', analysis_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis Result', new_analysis_result_path %>
